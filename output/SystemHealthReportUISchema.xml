  <QuerySection>
    <Query index="0" name="WorkstationCount" dtrange="0">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @configDB.WorkStationSettings
      </SQL>
    </Query>
    <Query index="1" name="ComServerCount" dtrange="0">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @configDB.MCom
      </SQL>
    </Query>
    <Query index="2" name="PanelCount" dtrange="0">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @configDB.Panel
      </SQL>
    </Query>
    <Query index="3" name="ComLossCount" dtrange="1">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @eventDB.Event
        WHERE Cat=6 and Status=1 @dateRange
      </SQL>
    </Query>
    <Query index="4" name="ReaderCount" dtrange="0">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @configDB.Reader
      </SQL>
    </Query>
    <Query index="5" name="BadgeCount" dtrange="0">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @configDB.Badge
      </SQL>
    </Query>
    <Query index="6" name="AccessCount" dtrange="1">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @eventDB.Event
        WHERE (Cat=0 OR Cat=8) @dateRange
      </SQL>
    </Query>
    <Query index="7" name="ValidAccessCount" dtrange="1">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @eventDB.Event
        WHERE (Cat=0 OR Cat=8) AND Status BETWEEN 1 AND 22 @dateRange
      </SQL>
    </Query>
    <Query index="8" name="AccessViolationCount" dtrange="1">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @eventDB.Event
        WHERE (Class LIKE '%BADGE Violate%' OR Class LIKE '%TAG Violate%') @dateRange
      </SQL>
    </Query>
    <Query index="9" name="BadgeAccessCount" dtrange="1">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @eventDB.Event
        WHERE Cat=0 @dateRange
      </SQL>
    </Query>
    <Query index="10" name="TagAccessCount" dtrange="1">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @eventDB.Event
        WHERE Cat=8 @dateRange
      </SQL>
    </Query>
    <Query index="11" name="ReaderMaxAccess" dtrange="1">
      <SQL>
        SELECT TOP 1 Name
        FROM @eventDB.Event
        WHERE (Cat=0 OR Cat=8) @dateRange
        GROUP BY DeviceNo, Name
        ORDER BY COUNT(*) DESC
      </SQL>
    </Query>
    <Query index="12" name="PanelMaxAccess" dtrange="1">
      <SQL>
        SELECT TOP 1 P.PanelName
        FROM @eventDB.Event as E INNER JOIN @configDB.Panel AS P on E.PnlNo = P.PnlNo
        WHERE (Cat=0 OR Cat=8) @dateRange
        GROUP BY P.PanelName
        ORDER BY COUNT(*) DESC
      </SQL>
    </Query>
    <Query index="13" name="ComMaxAccess" dtrange="1">
      <SQL>
        SELECT TOP 1 M.Description
        FROM @eventDB.Event as E INNER JOIN @configDB.Panel AS P on E.PnlNo = P.PnlNo INNER JOIN @configDB.MCom AS M on M.HCSNo = P.HCSNo
        WHERE (Cat=0 OR Cat=8) @dateRange
        GROUP BY M.Description
        ORDER BY COUNT(*) DESC
      </SQL>
    </Query>
    <Query index="14" name="ValidBadgeAccessCount" dtrange="1">
      <SQL>
        SELECT Total= COUNT(*)
        FROM @eventDB.Event
        WHERE (Cat=0)
        AND Status BETWEEN 1 AND 22 @dateRange
      </SQL>
    </Query>
    <Query index="15" name="ValidTagAccessCount" dtrange="1">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @eventDB.Event
        WHERE (Cat=8)
        AND Status BETWEEN 1 AND 22 @dateRange
      </SQL>
    </Query>
    <Query index="16" name="ReaderMaxValidAccess" dtrange="1">
      <SQL>
        SELECT TOP 1 Name
        FROM @eventDB.Event
        WHERE (Class LIKE '%BADGE Valid%' OR Class LIKE '%BADGE/TAG Valid%') @dateRange
        GROUP BY DeviceNo, Name
        ORDER BY COUNT(*) DESC
      </SQL>
    </Query>
    <Query index="17" name="BadgeAccessViolationCount" dtrange="1">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @eventDB.Event
        WHERE Class LIKE '%BADGE Violate%' @dateRange
      </SQL>
    </Query>
    <Query index="18" name="TagAccessViolationCount" dtrange="1">
      <SQL>
        SELECT Total=COUNT(*)
        FROM @eventDB.Event
        WHERE Class LIKE '%TAG Violate%'
      </SQL>
    </Query>
    <Query index="19" name="ReaderMaxAccessViolation" dtrange="1">
      <SQL>
        SELECT TOP (1) Name
        FROM @eventDB.Event
        WHERE (Class LIKE '%BADGE Violate%' OR Class LIKE '%TAG Violate%') @dateRange
        GROUP BY DeviceNo, Name
        ORDER BY COUNT(*) DESC
      </SQL>
    </Query>
    <Query index="20" name="ArchiveEventCount" dtrange="1">
      <SQL>
        SELECT Total=Count(*) FROM @ArchiveEventDB.Event
      </SQL>
    </Query>
    <Query index="21" name="LiveEventCount" dtrange="1">
      <SQL>
        SELECT Total=Count(*) FROM @eventDB.Event
      </SQL>
    </Query>
