<?xml version="1.0"?>
<configuration>
    <configSections>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="pepperdine.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
        </sectionGroup>
    </configSections>

    <applicationSettings>
        
        <pepperdine.Properties.Settings>
            <setting name="RunProcessAfterNoOfMinutes" serializeAs="String">
                <value>1</value>
            </setting>
            <setting name="RunErrorReportAfterNoOfMinutes" serializeAs="String">
                <value>1</value>
            </setting>
            <setting name="BadgeInfoFilePrefix" serializeAs="String">
                <value>BadgeInfo</value>
            </setting>
            <setting name="AccessInfoFilePrefix" serializeAs="String">
                <value>AccessInfo</value>
            </setting>
            <setting name="DataFolder" serializeAs="String">
                <value>C:\PeopleSoftData</value>
            </setting>
            <setting name="SqlSelectAccessInfo" serializeAs="String">
                <value>SELECT * FROM peoplesoft_Accessinfo WHERE cwid=@CWID</value>
            </setting>
            <setting name="SqlDeleteAccessInfo" serializeAs="String">
                <value>DELETE FROM peoplesoft_Accessinfo WHERE cwid=@CWID</value>
            </setting>
            <setting name="SqlGetAgName" serializeAs="String">
                <value>SELECT Description from MAccGrp where AgrpNo=@AgrpNo</value>
            </setting>
            <setting name="SqlAddUserField" serializeAs="String">
                <value> IF NOT EXISTS( SELECT * FROM UserFields WHERE Badge=@Badge AND @Facility=Facility AND PersonID=@PersonID AND UserFieldID=(SELECT UserFieldID FROM UserFieldDef WHERE UserFieldName=@FieldName) )
INSERT INTO UserFields(PersonID,Badge,Facility,UserFieldID,UserFieldValue,LastUpdated,caObjectID,LastWorkStation) 
 VALUES(@PersonID,@Badge,@Facility,(SELECT UserFieldID FROM UserFieldDef WHERE UserFieldName=@FieldName),@UserFieldValue,getutcdate(),newid(),@StatName)
 ELSE UPDATE UserFields SET UserFieldValue =@UserFieldValue, LastUpdated=GETUTCDATE(), LastWorkStation=@StatName 
 WHERE Badge=@Badge AND @Facility=Facility AND PersonID=@PersonID AND UserFieldID=(SELECT UserFieldID FROM UserFieldDef WHERE UserFieldName=@FieldName)
</value>
            </setting>
            <setting name="SqlUpdUserField" serializeAs="String">
                <value>UPDATE UserFields SET UserFieldValue =@UserFieldValue, LastUpdated=GETUTCDATE(), LastWorkStation=@StatName 
 WHERE Badge=@Badge AND UserFieldID=(SELECT UserFieldID FROM UserFieldDef WHERE UserFieldName=@FieldName)</value>
            </setting>
            <setting name="SqlUpdAccessInfo" serializeAs="String">
                <value>IF NOT EXISTS( SELECT * FROM peoplesoft_Accessinfo WHERE CWID=@CWID  ) 
 INSERT INTO peoplesoft_Accessinfo(CWID,HousingAccess,HousingActiveDate,HousingExpirationDate,HousingPatronType,ReportGenerated) 
 VALUES(@CWID,@HousingAccess,@HousingActiveDate,@HousingExpirationDate,@HousingPatronType,0) 
 ELSE UPDATE peoplesoft_Accessinfo SET HousingAccess=@HousingAccess, HousingActiveDate=@HousingActiveDate, 
 HousingExpirationDate=@HousingExpirationDate, HousingPatronType=@HousingPatronType, ReportGenerated=0 WHERE CWID=@CWID</value>
            </setting>
            <setting name="SqlSelBadgeForCwid" serializeAs="String">
                <value>select b.Badge, b.Facility, p.PersonId, p.SSN from badge as b, person as p where b.PersonID = p.PersonID and p.SSN=@CWID</value>
            </setting>
            <setting name="SqlGetDeptId" serializeAs="String">
                <value>SELECT DepartmentID FROM DepartmentInfo WHERE DepartmentName=@DepartmentName</value>
            </setting>
            <setting name="SqlGetBadgeCwid" serializeAs="String">
                <value>SELECT Badge FROM Badge INNER JOIN Person ON Person.PersonID = Badge.PersonID  WHERE Person.SSN=@SSN</value>
            </setting>
            <setting name="UserFieldPatronType" serializeAs="String">
                <value>Patron Type</value>
            </setting>
            <setting name="UserFieldRoomID" serializeAs="String">
                <value>RoomID</value>
            </setting>
            <setting name="UserFieldMoveInDate" serializeAs="String">
                <value>Move In Date</value>
            </setting>
            <setting name="UserFieldMoveOutDate" serializeAs="String">
                <value>Move Out Date</value>
            </setting>
            <setting name="UserFieldHousingImported" serializeAs="String">
                <value>Housing Imported</value>
            </setting>
            <setting name="UserFieldPatronCode" serializeAs="String">
                <value>Patron Code</value>
            </setting>
            <setting name="UserFieldProxNumber" serializeAs="String">
                <value>Prox Number</value>
            </setting>
            <setting name="UserFieldActivationDate" serializeAs="String">
                <value>Activation Date</value>
            </setting>
            <setting name="UserFieldExpirationDate" serializeAs="String">
                <value>Expiration Date</value>
            </setting>
            <setting name="UserFieldBadgeImported" serializeAs="String">
                <value>Badge Imported</value>
            </setting>
            <setting name="PartitionsToInsert" serializeAs="String">
                <value>Admin Partition|Campus</value>
            </setting>
            <setting name="SqlSelectPartitionInfo" serializeAs="String">
                <value>SELECT * FROM Partition WHERE PartitionName=@PartitionName</value>
            </setting>
            <setting name="SqlUpdUserFieldBadge" serializeAs="String">
                <value>update UserFields set Badge = @Badge, Facility = @Facility where PersonID = @PersonID</value>
            </setting>
        </pepperdine.Properties.Settings>
    </applicationSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
</configuration>
