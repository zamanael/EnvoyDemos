<?xml version="1.0"?>
<configuration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
	<system.runtime.remoting>
		<customErrors mode="Off" />
		<application name="CardAccess">
			<service>
				<wellknown mode="SingleCall" type="CardAccess.Mbl.MainBusinessLayer, CardAccess.Mbl" objectUri="Mbl" />
			</service>
			<channels>
				<channel ref="tcp" port="9001">
					<clientProviders>
						<formatter ref="binary" />
					</clientProviders>
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full" />
					</serverProviders>
				</channel>
				<channel ref="http" port="9005">
					<clientProviders>
						<formatter ref="binary" />
					</clientProviders>
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full" />
					</serverProviders>
				</channel>
			</channels>
		</application>
	</system.runtime.remoting>
</configuration>
